{
  "name": "car-shop",
  "version": "0.0.0",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "scripts": {
    "ng": "ng",
    "prestart": "node scripts/generate-env.js",
    "start": "dotenv -e .env -- ng serve",
    "prebuild": "node scripts/generate-env.js",
    "build": "dotenv -e .env -- ng build",
    "prewatch": "node scripts/generate-env.js",
    "watch": "dotenv -e .env -- ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint --fix",
    "format": "prettier --write './src/**/*.{ts,html,css,scss,json}'",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf node_modules dist && npm install",
    "check": "prettier --write  './src/**/*.{ts,html,css,scss,json}' && eslint --fix && tsc --noEmit "
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.12",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jwt-decode": "^4.0.0",
    "lucide-angular": "^0.553.0",
    "rxjs": "~7.8.0",
    "tailwind-merge": "^3.3.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.9",
    "@angular/cli": "^20.3.9",
    "@angular/compiler-cli": "^20.3.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^25.0.3",
    "angular-eslint": "20.5.2",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.3"
  }
}
